-->
<GridLayout class="page-content" loaded="onLoaded">
    <Label class="page-icon fa" text="&#xf002;"></Label>
    <Label class="page-placeholder" text="List from collection"></Label>
    <!-- <GridLayout> -->
        <ListView items="{{ memberList }}">
            <ListView.itemTemplate>
                <Label text="{{ member }}" horizontalAlignment="left" verticalAlignment="center"/>
            </ListView.itemTemplate>
        </ListView>
    <!-- </GridLayout> -->
</GridLayout>